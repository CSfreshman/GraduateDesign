<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.graduateDesign.dao.ScoreMapper">

    <select id="getByStuId" resultType="com.graduateDesign.entity.Score">
        select s.* from score s left join selected_topic st on s.selected_topic_id = st.id where st.stu_id = #{stuId}
    </select>

</mapper>
